Sv.LPlr.Idled:Connect(function()
	pcall(function()
		for i=15,0,-1 do
			svc("VirtualUser"):CaptureController()
			svc("VirtualUser"):ClickButton2(Vector2.new())
			WAIT(math.random(1,5))
		end
	end)

end)

