					notify(Plr.Name.." nolonger has deathexplosion enabled.",_A.R(1,1,0))
				else
					notify(Speaker.Name.." removed deathexplosion from "..tostring(Plr.Name),_A.R(1,1,0))
				end
			end
		end
	end,
