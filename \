		if v.Name~=Sv.LPlr.Name then
			if v.Team.Name~=Sv.LPlr.Team.Name then
				_A.O(HurtList,v)
			end
		end
	end
	LocalisedFunctions.hurt(HurtList)
	HurtList=_N -- Free up memory
end)
