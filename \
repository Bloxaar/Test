	local Anim=Sv.Ins("Animation")
	Anim.AnimationId="rbxassetid://178130996" -- Sit
	for i=0,3 do
		local T=Sv.LPlr[_A.P].Humanoid:LoadAnimation(Anim)
		T:Play()
		T:AdjustSpeed(0)
		T=_N
	end
	Anim.AnimationId="rbxassetid://175677527" -- ProneIdled
	for i=0,3 do
		local T=Sv.LPlr[_A.P].Humanoid:LoadAnimation(Anim)
		T:Play()
		T:AdjustSpeed(0)
		T=_N
	end
	Anim=_N
end)
