			dat=LocalisedFunctions.returnhostileplayers(dat)
		else
			if ishurtreq then
			else
				dat=LocalisedFunctions.returnhostileplayers(dat)
			end
		end
	end
	local A_1={}
	if ishurtreq==_T then
		for i,plr in Sv.P(dat)do
			if plr.Team.Name~=Sv.LPlr.Team.Name then
				if table.find(AddedWhitelists,plr.UserId)then else
