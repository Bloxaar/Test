			if Plr:FindFirstChild("SuperGun")~=_N then
				if Speaker==_N then
					notify(Plr.Name.." already has supergun enabled.",_A.R(1,1,0))
				end
			else
				Sv.Ins("BoolValue",Plr).Name="SuperGun"
