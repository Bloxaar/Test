})]]
					end
				end
			end
		end
	else
		local Char=nil
		local Humanoid=nil
		for i,plr in Sv.P(dat)do
			if table.find(AddedWhitelists,plr.UserId)then else
				Char=plr[_A.P]
				if Char~=_N then
					Humanoid=Char:FindFirstChildOfClass("Humanoid")
					if Humanoid~=_N then
						if Humanoid.Health <=0 then
						else
							for i=0,2 do
								_A.O(A_1,{
									["RayObject"]="",--Ray.new(Vector3.new(0,0,0),Vector3.new(0,0,0)),
